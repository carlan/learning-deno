{
    "files": [
        "main.ts"
    ],
    "quiet": false,
    "debug": true,
    "fullscreen": true,
    "extensions": [
        ".ts",
        ".json",
        ".html"
    ],
    "interval": 500,
    "watch": [
        "public/"
    ],
    "deno_args": [
        "--unstable",
        "--allow-read",
        "--allow-net"
    ],
    "execute": {
        ".ts": ["deno", "run"]
    },
    "fmt": false,
    "test": false
}